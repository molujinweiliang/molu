<swiper class='swiper' indicator-dots indicator-color='ragba(0,0,0,0.1)' indicator-active-color="#000" circular>
  <swiper-item wx:for="{{banner}}" wx:key>
    <image src='{{item}}' mode='aspectFill'></image>
  </swiper-item>
</swiper>
